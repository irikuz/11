# 1st program
print(9 ** 0.5 * 5)

# 2nd program
print(9.99 > 9.98 and 1000 != 1000.1)


# 3rd program
# Выводим результат 2 * 2 + 2 без приоритета
result1 = 2 * 2 + 2
print(result1)
# Выводим результат 2 * 2 + 2 с приоритетом для сложения
result2 = 2 * (2 + 2)
print(result2)
# Выводим результат сравнения двух выражений
comparison_result = result1 == result2
print(comparison_result)

# 4th program

# Дана строка
number_str = '123.456'
# Преобразуем строку в дробное число
number_float = float(number_str)
# Умножаем на 10, чтобы сместить цифру после точки в целую часть
number_shifted = number_float * 10  # 1234.56
# Применяем int() для получения целой части
integer_part = int(number_shifted)  # 1234
# Используем остаточное деление на 10, чтобы получить первую цифру после точки
first_digit_after_point = integer_part % 10  # 4
# Выводим результат
print(first_digit_after_point)
